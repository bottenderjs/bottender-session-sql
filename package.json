{
  "name": "bottender-session-sql",
  "description": "SQL Session Store for Bottender.",
  "license": "MIT",
  "version": "0.1.0",
  "main": "src/index.js",
  "bin": {
    "bottender-session-sql": "bin/bottender-session-sql"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint src examples",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "test": "npm run lint:fix && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit",
    "testonly:watch": "jest --watch",
    "preversion": "npm test"
  },
  "dependencies": {
    "cp-file": "^6.0.0",
    "knex": "^0.15.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-yoctol-base": "^0.17.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "pg": "^7.4.3",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "sqlite3": "^4.0.2"
  },
  "keywords": [
    "bottender",
    "knex",
    "session",
    "session-store",
    "sql"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
